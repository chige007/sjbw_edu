#topTips.alert(role='alert')
    span 用户名或密码错误
#confirmBox.modal.fade(tabindex='-1', role='dialog')
    .modal-dialog(role='document')
        .modal-content
            .modal-header
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                    span.glyphicon.glyphicon-remove(aria-hidden='true')
                h4.modal-title 系统提示
            .modal-body
                p.text
            .modal-footer
                button.btn.btn-default.cancel(type='button', data-dismiss='modal') 取消
                button.btn.btn-primary.confirm(type='button', data-dismiss='modal') 确定
